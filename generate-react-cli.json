{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss"
      },
      "path": "src/app/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "component": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss"
      },
      "path": "src/app/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateNamePage.tsx",
        "style": "templates/page/TemplateNamePage.module.scss",
        "lazy": "templates/page/TemplateNamePage.lazy.tsx"
      },
      "path": "src/app/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "hook": {
      "customTemplates": {
        "component": "templates/other/useTemplateName.ts"
      },
      "path": "src/app/hooks",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "slice": {
      "customTemplates": {
        "component": "templates/other/TemplateName.slice.ts"
      },
      "path": "src/app/state/slices",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}